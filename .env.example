#!/usr/bin/env bash
# Application specific environment variables

# shellcheck disable=SC2034
# https://github.com/koalaman/shellcheck/wiki/SC2034
# These variables are injected into the environment
#   and not necessarily used within this file

GITHUB_OWNER=shopsmart

# An admin personal access token with the `repo` scope
#   only required if attempting to set github secrets
GITHUB_TOKEN=

# AWS Credentials (preferred is a user with noop)
AWS_DEFAULT_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
